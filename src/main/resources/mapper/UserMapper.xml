<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zero.static_resources_manager.dao.UserMapper">
    <select id="selectByUserName" resultType="com.zero.static_resources_manager.dataobject.UserDao">
        select user_id, user_name, telephone
        from user_info
        where user_name = #{username}
    </select>






</mapper>